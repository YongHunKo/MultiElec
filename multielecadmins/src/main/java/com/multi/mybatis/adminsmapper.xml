<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multi.mapper.AdminsMapper">
	
	<select id="select" parameterType="String" resultType="AdminsDTO">
		SELECT * FROM admins WHERE id=#{id}
	</select>
	<select id="selectall" resultType="AdminsDTO">
		SELECT * FROM admins
	</select>
	<insert id="insert" parameterType="AdminsDTO">
		INSERT INTO admins VALUES (#{adminid},#{adminpwd},#{adminlev})
	</insert>
	<update id="update" parameterType="AdminsDTO">
		UPDATE admins SET adminpwd=#{adminpwd}, adminlev=#{adminlev} WHERE adminid=#{adminid}
	</update>
	<delete id="delete" parameterType="String">
		DELETE FROM cate WHERE adminid=#{adminid}
	</delete>
	
</mapper>